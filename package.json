{
  "name": "dainty-di",
  "version": "1.0.0",
  "description": "A mini Node.js dependency injection library",
  "keywords": [
    "dependency injection",
    "di",
    "ioc",
    "decorator",
    "typescript"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "docs:dev": "cd docs && npm run dev && cd -",
    "docs:build": "cd docs && npm run build && cd -",
    "build": "rimraf dist && tsc",
    "test": "jest --detectOpenHandles --forceExit",
    "test:cov": "jest --detectOpenHandles --forceExit --coverage",
    "test:ci": "jest --detectOpenHandles --forceExit --coverage --ci"
  },
  "author": "bLue",
  "license": "MIT",
  "dependencies": {
    "inversify": "^5.1.1",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@jest/globals": "^27.0.3",
    "@types/node": "^12",
    "jest": "^27.0.4",
    "rimraf": "^5.0.1",
    "ts-jest": "^27.0.3",
    "ts-node": "^9.1.1",
    "typescript": "^3.9.9"
  }
}
